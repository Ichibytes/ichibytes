{
  "name": "ichibytes",
  "version": "0.1.0",
  "description": "The official website of Ichibytes",
  "author": {
    "name": "Ezequiel Migueles",
    "url": "https://ichibytes.dev"
  },
  "license": "MIT",
  "homepage": "https://ichibytes.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ichibytes/ichibytes.git"
  },
  "bugs": {
    "url": "https://github.com/Ichibytes/ichibytes/issues"
  },
  "keywords": [
    "ichibytes",
    "personal-website",
    "portfolio",
    "nx",
    "monorepo",
    "typescript",
    "react",
    "nextjs",
    "vite",
    "nestjs",
    "prisma",
    "tailwindcss",
    "docker"
  ],
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:prod": "nx run-many --target=build --all --configuration=production",
    "test": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "lint": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "dev": "nx run-many --target=serve --all --parallel=3",
    "dev:api": "nx serve api",
    "dev:admin": "nx serve admin",
    "dev:web": "nx serve web",
    "dev:all": "nx run-many --target=serve --projects=api,admin,web --parallel=3",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options '{\"module\":\"commonjs\"}' libs/database/prisma/seed.ts",
    "docker:dev:up": "docker compose -f .docker/development/compose.yaml up -d",
    "docker:dev:down": "docker compose -f .docker/development/compose.yaml down",
    "docker:dev:logs": "docker compose -f .docker/development/compose.yaml logs -f",
    "docker:dev:restart": "docker compose -f .docker/development/compose.yaml restart",
    "security:audit": "npm audit --audit-level=moderate",
    "security:check": "npx audit-ci --moderate",
    "prepare": "husky"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "^22.3.3",
    "@nx/jest": "^22.3.3",
    "@nx/js": "22.3.3",
    "@nx/nest": "^22.3.3",
    "@nx/next": "^22.3.3",
    "@nx/node": "22.3.3",
    "@nx/react": "^22.3.3",
    "@nx/vite": "^22.3.3",
    "@nx/web": "22.3.3",
    "@nx/webpack": "^22.3.3",
    "@nx/workspace": "^22.3.3",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.19.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^4.0.0",
    "audit-ci": "^7.0.0",
    "autoprefixer": "^10.4.23",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-util": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "lint-staged": "^16.2.7",
    "nx": "22.3.3",
    "postcss": "^8.5.6",
    "prettier": "~3.6.2",
    "prisma": "^7.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.19",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "next": "^15.5.9",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "overrides": {
    "hono": "^4.11.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss,html}": [
      "prettier --write"
    ]
  },
  "prisma": {
    "seed": "ts-node libs/database/prisma/seed.ts"
  }
}
